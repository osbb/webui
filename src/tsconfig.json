{
  "compilerOptions": {
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "mapRoot": "./",
    "module": "es6",
    "moduleResolution": "node",
    "outDir": "../dist/out-tsc",
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [
      "../node_modules/@types"
    ],
    "types": [
      "core-js",
      "hammerjs",
      "jasmine",
      "node",
      "node-uuid",
      "protractor",
      "socket.io-client",
      "source-map"
    ]
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
